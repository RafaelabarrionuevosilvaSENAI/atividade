function  calcularFatorial(numFatorial) {
   let result = 1;
if(numFatorial < 1) {
 return { "erro": "Input menor que 1"}
}
for (let i = numFatorial; i >= 1; i--) {
  result *= i;
}
return result; //número
//console.log(`A mutiplicação é: ${mutiplicação}`);
}
console.log(calcularFatorial(numFatorial));
